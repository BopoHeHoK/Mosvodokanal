        <item name="android:textSize">@dimen/text_md</item>
        <item name="android:textColor">@color/text_secondary_light_color</item>
    </style>

    <style name="Title" parent="TextSemiBold">
        <item name="fontFamily">@font/sf_pro_display_bold</item>
        <item name="android:textColor">@color/text_base_color</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="TextOnToolbar" parent="TextBase">
        <item name="android:textColor">@color/text_toolbar</item>
        <item name="android:textSize">@dimen/text_md</item>
    </style>

    <style name="EditText" parent="TextBase">
        <item name="android:textColor">@color/text_primary_light_color</item>
